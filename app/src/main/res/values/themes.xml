<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Saget" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="buttonEditInfoTI" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorAccent">#09FFA6</item>
        <item name="android:radius">7dp</item>
    </style>
    <style name="buttonVerMasSoliTi" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">#206CEF</item>
        <item name="colorPrimaryVariant">#2E65D1</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="android:radius">7dp</item>
    </style>
    <style name="buttonAceptarSoliTi" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">#00338A</item>
        <item name="colorPrimaryVariant">#053187</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="android:radius">7dp</item>
    </style>
    <style name="buttonDenegarSoliTi" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">#E60808</item>
        <item name="colorPrimaryVariant">#AA0202</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="android:radius">7dp</item>
    </style>

    <style name="buttonDeleteInfoTI" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorAccent">#FF0909</item>
        <item name="android:radius">7dp</item>
    </style>
    <style name="Theme.Saget.Registro" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#000000</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="BottomNavigation">
        <item name="android:background">@color/black</item>
        <item name="itemTextColor">@color/white</item>
    </style>



    <style name="MyButtonStyle" parent="Widget.MaterialComponents.Button">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay.MyApp.Button.Rounded</item>
    </style>

    <style name="ShapeAppearanceOverlay.MyApp.Button.Rounded" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>
    </style>

</resources>